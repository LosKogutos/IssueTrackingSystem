@model IEnumerable<IssueTrackingSystem.Models.Space>

@{
    ViewBag.Title = "Index";
}

<h2>My Spaces</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Open", "Cardwall", new { spaceName = item.Name }) 
        </td>
    </tr>
}

</table>
